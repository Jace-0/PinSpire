{
  "name": "backend",
  "version": "1.0.0",
  "description": "PinSpire Backend",
  "main": "start.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node start.js",
    "dev": "cross-env NODE_ENV=development nodemon start.js",
    "test": "cross-env NODE_ENV=test node ",
    "test:supertest": "cross-env NODE_ENV=test node --test --test-concurrency=1 ",
    "db:init": "cross-env NODE_ENV=test node scripts/init-db.js",
    "start:test": "cross-env NODE_ENV=test nodemon start.js",
    "build:ui": "rm -rf build && cd ../frontend/ && npm run build && cp -r build ../backend",
    "build:ui:dev": "rm -rf build && cd ../frontend/ && npm run build:dev && cp -r build ../backend",
    "lint": "eslint",
    "lint:fix": "eslint . --fix"
  },
  "author": "Jace",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.18.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "umzug": "^3.8.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@stylistic/eslint-plugin-js": "^3.0.0",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.2.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  }
}
